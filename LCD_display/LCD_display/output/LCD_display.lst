   818  2FDA  21 E8                and  A,(UART_Sensor_RX_ERROR|UART_Sensor_RX_REG_FULL)
   819  2FDC  08                   push A
   820  2FDD  21 08                and  A,UART_Sensor_RX_COMPLETE                          ; Check if a character is ready
   821  2FDF  B007                 jnz  .RX_GET_DATA                                       ; Data Ready go read it.
   822  2FE1  18                   pop  A
