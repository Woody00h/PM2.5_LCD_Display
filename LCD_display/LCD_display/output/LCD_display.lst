   824  2FE4  4B                   swap A,X
   825  2FE5  8007                 jmp  End_UART_Sensor_iReadChar
   826                          
   827  2FE7                    .RX_GET_DATA:
   828  2FE7  18                   pop  A
   829  2FE8  21 E0                and  A,UART_Sensor_RX_ERROR
   830  2FEA  4B                   swap A,X
   831  2FEB  5D 2E                mov  A,REG[UART_Sensor_RX_BUFFER_REG ]                  ; Read data first, then
   832                                                                                     ; determine if data is valid
   833                          
   834  2FED                     End_UART_Sensor_iReadChar:
   835                             RAM_EPILOGUE RAM_USE_CLASS_1
   836  2FED  7F                   ret
   837                          .ENDSECTION
